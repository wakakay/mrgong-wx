
page {background-color: #f5f5f5}
.ui-help-list {margin-top: 20rpx;background-color: #ffffff;}
.ui-item{display: flex;height: 112rpx;align-items:center;font-size: 32rpx;}
.ui-help-list image{width: 36rpx;height: 36rpx;margin: 0 30rpx}
.ui-title { position: relative;width: 100%;height: 112rpx;display: flex;align-items: center;}
.ui-item-narrow {position: absolute;right: 30rpx;top: 50%;transform: translate(0,-50%)}
.ui-item-narrow image{width: 20rpx;height: 32rpx;}
