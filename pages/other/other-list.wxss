page {background-color: #f5f5f5}
.ui-user-box {padding:40rpx 40rpx 60rpx;display: flex;justify-content:space-between;background-color: #f5f5f5}
.ui-user-box .ui-user-name {display: flex;flex-direction: column}
.ui-user-name p{font-size: 32rpx;color:#999999;}
.cell-login{font-size: 50rpx;color: #2e2e2e;margin-bottom: 10rpx;font-weight: 600}
.ui-user-box .ui-user-topic {width: 134rpx;height: 134rpx;display: flex;border-radius: 50%;position: relative;background-color: #fff;}
.ui-user-topic image {width: 124rpx;height: 124rpx;border-radius: 50%;position: absolute;left: 50%;top: 50%;transform: translate(-50%,-50%)}
.ui-tab-list{background-color: #fff;margin: 0 20rpx;height: 294rpx;border-radius: 20rpx;padding: 40rpx 0;box-shadow:1rpx 10rpx 4rpx rgba(82,90,99,0.05);}
.ui-list-low {display: flex;}
.ui-list-low:last-child {margin-top: 40rpx}
.ui-list-box {display: flex;flex-direction: column;align-items:center;width: 236rpx;}
/* .ui-list-box:nth-child(2){width: 400rpx;} */
.ui-list-box text{font-size: 28rpx;color: #2e2e2e}
.ui-list-box image{width: 94rpx;height: 94rpx;}
.ui-system-list {background-color: #fff;margin-top: 40rpx}
.ui-system-list image {width: 48rpx;height: 48rpx;margin:18rpx 40rpx 0;}
.ui-system-li {height: 110rpx; display: flex;align-items:center;}
.ui-system-li .ui-li-title{height: 110rpx;display: flex;align-items:center;width:100%;position: relative;font-size: 32rpx}
.ui-li-title .ui-li-narrow{position: absolute;right: 40rpx;top:50%;transform: translate(0,-50%)}
.ui-li-title .ui-li-narrow image{width: 20rpx;height: 28rpx; margin: 0;}
.ui-system-li:first-child .ui-li-title {border-bottom: 1rpx solid #eeeeee;}